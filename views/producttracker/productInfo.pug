extends ../layout-admin

block content
    div(class="container")
        div(class="row")
            div(class="col-lg-12")
                div(class="cards")
                        div(class="container")
                            div(class="page-title")
                                h3 Tracking Id : #{tracks[0].id}
                            div(class="row")
                                div(class="col-lg-12")
                                    .row
                                    for item in tracks
                                        .row
                                        if(tracks)
                                        each item in tracks
                                            include trackpage